            <!-- the template for scenes -->
            <file category="sourceC"     name="Helper/template/arm_2d_scene_template.c" attr="template"  select="User Scene Template"   version="1.2.8" />
            <file category="header"      name="Helper/template/arm_2d_scene_template.h" attr="template"  select="User Scene Template"   version="1.2.8" />

            <!-- the template for user defined OPCODEs -->
